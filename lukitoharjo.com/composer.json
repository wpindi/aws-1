{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "composer", "roots", "wordpress", "wp", "wp-config"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
    },
    {
      "type": "vcs",
      "url": "git@github.com:wpindi/billey.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:wpindi/revslider.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:wpindi/insight-core.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:wpindi/elementor-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:wpindi/wp-rocket.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:wpindi/wp-rocket-no-cache.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:wpindi/soil-activate.git"
    }
  ],
  "require": {
    "php": ">=7.1",
    "composer/installers": "^1.8",
    "vlucas/phpdotenv": "^4.1.3",
    "oscarotero/env": "^1.2.0",
    "roots/wordpress": "5.4.1",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/elementor":"2.9.14",
    "wpackagist-plugin/seo-by-rank-math":"1.0.46",
    "wpackagist-plugin/header-footer":"3.2.1",
    "wpackagist-plugin/shortpixel-image-optimiser":"4.19.3",
    "wpindi/billey": "master",
    "wpindi/revslider": "master",
    "wpindi/insight-core": "master",
    "wpindi/elementor-pro": "master",
    "wpindi/wp-rocket": "master",
    "wpindi/wp-rocket-no-cache": "master",
    "wpindi/soil-activate": "master",
    "roots/soil": "^3.9"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5.4",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
